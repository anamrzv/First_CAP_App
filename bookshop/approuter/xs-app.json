{
  "welcomeFile": "/admin/index.html",
  "authenticationMethod": "none",
  "routes": [
      {
	      "source": "^/browse/(.*)$",
	      "destination": "srv-api",
        "authenticationType": "none",
	      "csrfProtection": false,
        "cacheControl": "no-cache, no-store, must-revalidate"
      },
      {
	      "source": "^/adminapi/(.*)$",
	      "destination": "srv-api",
        "authenticationType": "none",
	      "csrfProtection": false,
        "cacheControl": "no-cache, no-store, must-revalidate"
      },
      {
	      "source": "^(.*)$",
	      "service": "html5-apps-repo-rt",
        "authenticationType": "none",
	      "csrfProtection": false,
        "cacheControl": "no-cache, no-store, must-revalidate"
      }
    ]
}