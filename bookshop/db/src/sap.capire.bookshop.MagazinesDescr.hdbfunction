FUNCTION SAP_CAPIRE_BOOKSHOP_MAGAZINESDESCR
  RETURNS TABLE (ID integer, MAGAZINE_DESCR nvarchar(2000))
  LANGUAGE SQLSCRIPT
  AS
BEGIN
  RETURN SELECT 
           100 + ID AS ID,
           'From my HANA function: A summary for the magazine ' || TITLE  || ': ' || DESCR AS MAGAZINE_DESCR
         FROM SAP_CAPIRE_BOOKSHOP_MAGAZINES;
END